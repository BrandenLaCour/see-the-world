<%- include('../partials/header.ejs')%>


<h1>See All the Photos</h1>

<% for(let i = 0; i < articles.length; i++) {%>
				
<div class="card mt-10" style="width: 35rem;">
	<a href="/articles/<%=articles[i].id%>" >
	<img src="<%=cloudinary.url(articles[i].imageId)%>" class="card-img-top" alt="..."> 
		
	</a>
  <div class="card-body">
  	<p><%=articles[i].title%></p>
  	<p><%=articles[i].date.toLocaleString()%></p>
  </div>
</div>
	
<%}%>


<%- include('../partials/footer.ejs')%>